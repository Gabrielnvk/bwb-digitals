    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.3",
    "@types/react": "~19.0.14",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@/src/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-clone-referenced-element|@react-navigation|expo(nent)?|expo-modules-core|@expo(nent)?/.*|expo-router|@expo/.*|@unimodules/.*|unimodules|sentry-expo|nativewind|react-native-css-interop|react-native-svg|@rn-primitives/.*)/)"
    ]
  },
  "resolutions": {
    "lightningcss": "1.27.0"
  },
  "private": true
}
